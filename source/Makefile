	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: kangoroo-mac.pro  /Users/misha/qt3-release-static/mkspecs/default/qmake.conf ../../../qt3-release-static/lib/libqt.prl
	$(QMAKE) -o Makefile kangoroo-mac.pro
qmake: 
	@$(QMAKE) -o Makefile kangoroo-mac.pro

kangoroo.app/Contents/MacOS/../PkgInfo: 
	@test -d kangoroo.app/Contents/MacOS/ || mkdir -p kangoroo.app/Contents/MacOS/
	@$(DEL_FILE) kangoroo.app/Contents/MacOS/../PkgInfo
	@echo "APPL????" >kangoroo.app/Contents/MacOS/../PkgInfo
kangoroo.app/Contents/MacOS/../Info.plist: 
	@test -d kangoroo.app/Contents/MacOS/ || mkdir -p kangoroo.app/Contents/MacOS/
	@$(DEL_FILE) kangoroo.app/Contents/MacOS/../Info.plist
	@sed -e "s,@ICON@,application.icns,g" -e "s,@EXECUTABLE@,kangoroo,g" "/Users/misha/qt3-release-static/mkspecs/default/Info.plist.app" >"kangoroo.app/Contents/MacOS/../Info.plist"
kangoroo.app/Contents/MacOS/../Resources/application.icns: ../icon/kangoroo.icns
	@test -d kangoroo.app/Contents/MacOS/../Resources/ || mkdir -p kangoroo.app/Contents/MacOS/../Resources/
	@$(DEL_FILE) kangoroo.app/Contents/MacOS/../Resources/application.icns
	@$(COPY_FILE) ../icon/kangoroo.icns kangoroo.app/Contents/MacOS/../Resources/application.icns
dist: 
	@mkdir -p .tmp/kangoroo && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/kangoroo/ && ( cd `dirname .tmp/kangoroo` && $(TAR) kangoroo.tar kangoroo && $(GZIP) kangoroo.tar ) && $(MOVE) `dirname .tmp/kangoroo`/kangoroo.tar.gz . && $(DEL_FILE) -r .tmp/kangoroo

