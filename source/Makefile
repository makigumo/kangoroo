####### Build rules

all: Makefile kangoroo.app/Contents/MacOS/../PkgInfo kangoroo.app/Contents/MacOS/../Info.plist kangoroo.app/Contents/MacOS/../Resources/application.icns $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	test -d kangoroo.app/Contents/MacOS/ || mkdir -p kangoroo.app/Contents/MacOS/
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

